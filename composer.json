{
    "name": "tavvet/grid-bundle",
    "description": "Datagrid for symfony2 or symfony3 or symfony4 or symfony5",
    "keywords": ["grid", "jQGrid", "table"],
    "type": "symfony-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "David Tee"
        },
        {
            "name": "Matthew J Mucklo",
            "email": "mmucklo@gmail.com"
        },
        {
            "name": "Anton Rudakov",
            "email": "anton.rudakov@protonmail.com"
        }
    ],
    "require": {
        "php": ">=8.0",
        "symfony/framework-bundle": "5.*|6.*",
        "sensio/framework-extra-bundle": "5.*|6.*",
        "symfony/templating": "5.*|6.*",
        "symfony/translation": "5.*|6.*",
        "symfony/twig-bundle": "5.*|6.*",
        "twig/twig": "^1.0||^2.0|^3.0"
    },
    "config": {
        "bin-dir": "bin"
    },
    "autoload": {
        "psr-4": { "Dtc\\GridBundle\\": "" },
        "exclude-from-classmap": [
            "/Tests/"
        ]
    }
}
